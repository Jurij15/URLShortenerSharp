@page "/NotFound"
@rendermode InteractiveServer
@inject NavigationManager Navigation
@inject HttpClient Http

<div class="page-center">
    <h1>Not Found</h1>
    <p>The requested URL was not found!</p>

    <FluentButton OnClick="@GoHome">
        Go Back Home
    </FluentButton>
</div>

@code {
    private void GoHome()
    {
        Navigation.NavigateTo("/");
    }
}
